generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id          BigInt   @id @default(autoincrement())
  title       String
  description String?
  price       Int
  location    String
  beds        Int
  baths       Int
  sqft        Int
  category    String   // <--- MAKE SURE THIS IS HERE
  image       String?  
  gallery     String[] // <--- THIS IS THE NEW ONE
  user_id     String   // <--- MAKE SURE THIS IS HERE
  createdAt   DateTime @default(now())
}